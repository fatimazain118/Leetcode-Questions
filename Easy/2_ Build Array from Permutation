class Solution {
public:
    vector<int> buildArray(vector<int>& nums) {
        
        vector<int> result;
        for(int i=0; i<nums.size(); i++)
        {
            int a = nums[nums[i]];
            result.push_back(a);
        }
    
        return result;
    }
                           
};

Taking no extra space O(1)
