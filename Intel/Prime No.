  int isPrime(int N){
        // code here
        //int flag = 0;
        
        if(N<=1) return 0;
        
        for(int i=2; i<N/2; i++){
            if(N%i == 0){
                // flag = 1;
                // break;
                return 0;
            }
        }
        //if(flag) return 0;
        return 1;
    }
};
